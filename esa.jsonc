{
  "build": {
    "command": "npm run build",
    "output": "dist"
  },
  "functions": {
    "pattern": "dist/**/*.js",
    "exclude": "dist/service-worker.js"
  },
  "assets": {
    "directory": "dist",
    "include": [
      "**/*.html",
      "**/*.css",
      "**/*.js",
      "**/*.json",
      "**/*.png",
      "**/*.jpg",
      "**/*.svg",
      "**/*.ico",
      "**/*.webmanifest"
    ]
  }
}
